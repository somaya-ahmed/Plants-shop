    private void jCheckBox15ActionPerformed(java.awt.event.ActionEvent evt) {                                            
      
    doconnect();
    
     try{
         query="select * from product where p_id =9";
       resultset = statement.executeQuery(query);
       resultset.next();
       String name = resultset.getString("name");
     amount = Integer.parseInt(jTextField15.getText());
     int cost = resultset.getInt("cost") * Integer.parseInt(jTextField15.getText());
    String id =resultset.getString("p_id");
     
    query="select * from cart";
    resultset = statement.executeQuery(query);
     resultset.next();
     currentRow= resultset.getRow();
     resultset.moveToInsertRow();
     resultset.updateInt("no", no);
     resultset.updateString("name", name);
     resultset.updateString("C_ID", id);
     resultset.updateInt("amount", amount);
     resultset.updateInt("cost", cost);
     
     resultset.insertRow();
      addedrow = resultset.getRow();
     resultset.close();
     statement.close();
     
     }catch (SQLException ex){
     JOptionPane.showMessageDialog(this, ex);
     }
     
    }                                           

    private void jTextField15ActionPerformed(java.awt.event.ActionEvent evt) {                                             
