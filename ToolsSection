    private void jTextField18ActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
    }                                            

    private void jCheckBox18ActionPerformed(java.awt.event.ActionEvent evt) {                                            
       doconnect();
        if(jCheckBox18.isSelected()){
     try{ 
         query="select * from product where p_id = 500 ";
     resultset = statement.executeQuery(query);
     resultset.next();
     String name = resultset.getString("name");
     amount = Integer.parseInt(jTextField18.getText());
     int cost = Integer.parseInt(jTextField18.getText()) * (resultset.getInt("cost"));
     String id = resultset.getString("p_id");
     query="select * from cart";
     resultset=statement.executeQuery(query);
     resultset.next();
     currentRow= resultset.getRow();
     resultset.moveToInsertRow();
      resultset.updateInt("no", no);
     resultset.updateString("C_ID", id);
     resultset.updateString("name", name);
     resultset.updateInt("amount", amount);
     resultset.updateInt("cost", cost);
     
     resultset.insertRow();
     
     resultset.close();
     statement.close();
       
     }catch (SQLException ex){
     JOptionPane.showMessageDialog(this, ex);
     }
        }
     else 
     if(!jCheckBox18.isSelected()){
        doconnect();
     try{
     resultset = statement.executeQuery("select * from cart where C_ID =500");
     resultset.next();
     resultset.deleteRow();
     resultset.close();
     statement.close();
     }
         
        catch (SQLException ex){
     JOptionPane.showMessageDialog(this, ex);
     }
     }
    }                                           

