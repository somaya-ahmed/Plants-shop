    private void jButton15ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        amount= Integer.parseInt(jTextField8.getText());
        if(amount>0)amount-=1; 
            else    amount=0;
        jTextField8.setText(Integer.toString(amount));
    }                                         

    private void jTextField8ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
    }                                           

    private void jCheckBox8ActionPerformed(java.awt.event.ActionEvent evt) {                                           
         doconnect();
        if(jCheckBox8.isSelected()){
     try{ 
         query="select * from product where p_id = 100 ";
     resultset = statement.executeQuery(query);
     resultset.next();
     String name = resultset.getString("name");
     amount = Integer.parseInt(jTextField8.getText());
     int cost = amount * (resultset.getInt("cost"));
     String id = resultset.getString("p_id");
     query="select * from cart";
     resultset=statement.executeQuery(query);
     resultset.next();
     currentRow= resultset.getRow();
     resultset.moveToInsertRow();
      resultset.updateInt("no", no);
     resultset.updateString("C_ID", id);
     resultset.updateString("name", name);
     resultset.updateInt("amount", amount);
     resultset.updateInt("cost", cost);
     
     resultset.insertRow();
     
     resultset.close();
     statement.close();
       
     }catch (SQLException ex){
     JOptionPane.showMessageDialog(this, ex);
     }
        }
     else 
     if(!jCheckBox8.isSelected()){
        doconnect();
     try{
    resultset = statement.executeQuery("select * from cart where C_ID =100");
     resultset.next();
     resultset.deleteRow();
     resultset.close();
     statement.close();
     }
         
        catch (SQLException ex){
     JOptionPane.showMessageDialog(this, ex);
     }
     }
    
    }                                          

